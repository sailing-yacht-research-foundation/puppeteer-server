{
  "name": "puppeteer-server",
  "version": "0.0.1",
  "description": "Puppeteer Server",
  "main": "src/main.ts",
  "scripts": {
    "build": "rm -rf ./build && tsc --project ./ && npm run copy-files",
    "copy-files": "cp -r ./src/assets/ ./build/assets/",
    "start": "node build/main.js",
    "dev": "tsnd --respawn --poll src/main.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest --coverage --detectOpenHandles --forceExit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1029.0",
    "bull": "^4.1.1",
    "cheerio": "*",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ioredis": "^4.28.0",
    "jimp": "^0.16.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^11.0.0",
    "sequelize": "^6.9.0",
    "sharp": "^0.29.3",
    "striptags": "^3.2.0",
    "supertest": "^6.1.6",
    "uuid": "^3.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/bull": "^3.15.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.7",
    "@types/sharp": "^0.29.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "babel-jest": "^27.3.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
